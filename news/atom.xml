---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Blog</title>
	<link href="http://merryndd.github.io/news/atom.xml" rel="self"/>
	<link href="http://merryndd.github.io/news"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://merryndd.github.io/news</id>
	<author>
		<name>Merryn</name>
		<email>merryndd@gmail.com</email>
	</author>

	{% for post in site.categories.blog %}
	{% if post.url %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://merryndd.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://merryndd.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endif %}
	{% endfor %}

</feed>
